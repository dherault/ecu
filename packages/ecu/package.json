{
  "name": "@ecu/client",
  "version": "1.0.0",
  "description": "A shield",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "start:watch": "nodemon --watch src/server --ext ts,tsx,snap --exec \"npm start\"",
    "dev": "npm run clean:build && nodemon --watch src --ext ts,tsx,snap --exec \"npm run build\"",
    "build": "npx tsc --declaration",
    "clean": "npm run clean:build && npm run clean:node-modules && npm i",
    "clean:node-modules": "rimraf node_module package-lock.json",
    "clean:build": "rimraf dist",
    "clean:peerDependencies": "rimraf node_modules/react node_modules/react-dom",
    "test": "echo \"Error: no test specified\" && exit 0",
    "test:watch": "nodemon --watch src --watch tests --ext ts,tsx,snap --exec \"npm run test\"",
    "prepublishOnly": "npm run clean && npm test && npm run build"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@emotion/react": ">=10.0.0",
    "@emotion/styled": ">=10.0.0",
    "honorable": "*",
    "honorable-theme-default": "*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.9",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0"
  },
  "dependencies": {
    "@apollo/client": "^3.6.5",
    "@babel/core": "^7.18.2",
    "@babel/generator": "^7.18.2",
    "@babel/parser": "^7.18.4",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/traverse": "^7.18.2",
    "@babel/types": "^7.18.4",
    "apollo-server": "^3.8.1",
    "eslint": "^8.16.0",
    "graphql": "^16.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "typescript": "^4.7.2"
  }
}
